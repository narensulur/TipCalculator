{
  "name": "QBO Calendar",
  "description": "Quickbooks with Google Calendar",
  "manifest_version": 2,
  "default_locale":"en",
  "options_page": "views/options.html",
  "options_ui": {
    "page": "views/options.html",
    "chrome_style": true
  },
  "version": "1.0.0",
  "background": {
    "page": "background.html"
  },
  "applications": {
    "gecko": {
      "id": "Alexander_Shive@intuit.com",
      "strict_min_version": "45.0.0"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://calendar.google.com/*"],
      "css": ["styles/style.css"],
      "js": [
        "javascript/lib/jquery.min.js", 
        "javascript/lib/jquery.autocomplete.min.js", 
        "javascript/lib/common.js",
        "javascript/cal.js"
      ]
    }
  ],
  "permissions": [
    "notifications",
    "activeTab",
    "storage",
    "tabs",
    "https://www.google.com/m8/feeds/*",
    "http://calendar.google.com/calendar/*", 
    "https://calendar.google.com/calendar/*",
    "https://accounts.google.com/o/oauth2/token",
    "https://*.googleapis.com/*",
    "https://apis.google.com/js/*"
  ],
  "icons": {
      "128": "images/icon-128.png",
      "16": "images/icon-16.png",
      "48": "images/icon-48.png"
   },
   "content_security_policy": "script-src 'self' https://www.google.com https://apis.google.com https://calendar.google.com https://ssl.google-analytics.com; object-src 'self'",
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "QBO Calendar",
    "default_popup": "views/popup.html"
  },
  "icons": {
    "128": "images/icon-128.png",
    "48": "images/icon-48.png",
    "16":"images/icon-16.png"
  }
}
